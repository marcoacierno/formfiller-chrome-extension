{
    "name": "form-filler",
    "version": "1.0",
    "description": "Form filler",
    "manifest_version": 2,
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_title": "Form Filler",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "data/person.js",
                "data/lorem.js",
                "filler.js"
            ]
        }
    ]
}